{
  "name": "wekwekcode",
  "displayName": "WEK WEK CODE",
  "description": "THE WEK WEK CODE SHORTCUT",
  "publisher": "CodeSaint",
  "author": {
    "name": "Teguh Saint",
    "url": "https://instagram.com/teguh.saint"
  },
  "contributors": [
    {
      "name": "Naufal",
      "url": "https://instagram.com/noval_amirul"
    }
  ],
  "icon": "fav.png",
  "repository": {
    "url": "https://github.com/teguhsaint/wekwekcode.git"
  },
  "version": "1.0.5",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:wekwekcode.duck_start"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wekwekcode.duck_start",
        "title": "CI4 Template",
        "category": "CI4"
      },
      {
        "command": "wekwekcode.ci_controller",
        "title": "CI4 Controller",
        "category": "CI4"
      },
      {
        "command": "wekwekcode.ci_routes",
        "title": "CI4 Routes",
        "category": "CI4"
      },
      {
        "command": "wekwekcode.downloadcss_orjs",
        "title": "Download CSS or JS",
        "category": "WekCode"
      },
      {
        "command": "wekwekcode.ci_view_extend",
        "title": "Buat View Extend",
        "category": "CI4"
      }
    ],
    "keybindings": [
      {
        "command": "wekwekcode.rutein",
        "key": "CTRL+SHIFT+d"
      }
    ],
    "snippets": [
      {
        "language": "php",
        "path": "snippet/ci4.json"
      },
      {
        "language": "html",
        "path": "snippet/ci4.json"
      }
    ],
    "icons": {
      "my-duck": {
        "description": "myduck",
        "default": {
          "fontPath": "./icons/e800.woff",
          "fontCharacter": "\\e806"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {
    "decompress": "^4.2.1"
  },
  "__metadata": {
    "id": "89fce65e-2906-4587-a652-1103cba84b91",
    "publisherDisplayName": "Code Saint",
    "publisherId": "aabae9aa-3a6b-4247-82d6-0799ebf2416c",
    "isPreReleaseVersion": false
  }
}
